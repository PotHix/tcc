LATEX = latex
PDFLATEX = pdflatex
BIBTEX = bibtex


TARGET=seg_info_contratos
TRASH = *.aux *.log *.toc *~ *.blg *.bbl *.lof *.lot
RM = rm


pdf:
	$(PDFLATEX) $(TARGET)
	$(BIBTEX) $(TARGET)
	$(PDFLATEX) $(TARGET)
	$(PDFLATEX) $(TARGET)
	$(PDFLATEX) $(TARGET)

clean:
	$(RM) $(TRASH)

clean-all: clean
	$(RM) *.pdf

